<p id="notice"><%= notice %></p>

<h1>Event2s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event2s.each do |event2| %>
      <tr>
        <td><%= event2.title %></td>
        <td><%= event2.description %></td>
        <td><%= event2.start_time %></td>
        <td><%= event2.end_time %></td>
        <td><%= link_to 'Show', event2 %></td>
        <td><%= link_to 'Edit', edit_event2_path(event2) %></td>
        <td><%= link_to 'Destroy', event2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event2', new_event2_path %>
